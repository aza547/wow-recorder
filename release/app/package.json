{
  "name": "WarcraftRecorder",
  "version": "2.8.1",
  "description": "A World of Warcraft screen recorder",
  "main": "./dist/main/main.js",
  "author": {
    "name": "Alex K"
  },
  "scripts": {
    "electron-rebuild": "node -r ts-node/register ../../.erb/scripts/electron-rebuild.js",
